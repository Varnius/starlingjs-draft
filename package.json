    "null-loader": "^0.1.1",
    "rimraf": "^2.5.3",
    "sinon": "^4.0.2",
    "sinon-chai": "^2.14.0",
    "webpack": "3.7.1",
    "webpack-dev-middleware": "^1.12.0"
  },
