{
  "name": "starling-js",
  "private": true,
  "version": "1.0.0",
  "description": "StarlingJS",
  "main": "./src/index.js",
  "scripts": {
    "compile": "NODE_ENV=production rimraf dist && NODE_ENV=production webpack --progress --config webpack.config.prod.js",
    "start": "node server.js",
    "build": "npm run compile",
    "test": "eslint src && NODE_ENV=production mocha  -r babel-register './tests/tests.js' './tests/**/*.spec.js' --timeout 1000",
    "test:watch": "NODE_ENV=production mocha  -r babel-register './tests/tests.js' './tests/**/*.spec.js' --reporter nyan --watch --timeout 1000"
  },
  "author": "Simonas Pauliukeviƒçius",
  "devDependencies": {
    "babel-core": "^6.10.4",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-es2017": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^4.1.2",
    "chalk": "^1.1.3",
    "eslint": "^3.6.0",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-import": "^1.10.2",
    "express": "^4.14.0",
    "file-loader": "^0.9.0",
    "json-loader": "^0.5.4",
    "mocha": "^4.0.1",
    "nock": "^9.1.5",
